  "scripts": {
    "short_start": "nodemon ./backend/server.ts",
    "start": "nodemon --delay 500ms ./backend/server.ts & webpack-dev-server",
    "start_backend_frontend_webpackdevserver": "nodemon --delay 500ms ./backend/server.ts & webpack --watch --hot & webpack-dev-server",
    "start_backend_frontend": "nodemon --delay 500ms ./backend/server.ts & webpack --watch --hot",
    "start_backend": "nodemon --delay 500ms ./backend/server.ts",
    "start_frontend": "node node_modules/webpack-cli/bin/cli.js --progress --watch"
  },
